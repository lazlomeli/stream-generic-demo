# Stream API Credentials
STREAM_API_KEY=your_stream_api_key_here
STREAM_API_SECRET=your_stream_api_secret_here
VITE_STREAM_API_KEY=your_stream_api_key_here
VITE_STREAM_API_SECRET=your_stream_api_secret_here

# Auth0 Configuration
VITE_AUTH0_DOMAIN=dev-xxxxx.us.auth0.com
VITE_AUTH0_AUDIENCE=your_auth0_api_url
VITE_AUTH0_CLIENT_ID=your_auth0_client_id
VITE_AUTH0_CLIENT_SECRET=your_client_secret_here

# Note: VITE_AUTH0_DOMAIN is required for the Auth0Provider to work
