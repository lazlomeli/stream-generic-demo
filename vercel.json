{
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/stream/feed-actions.ts": {
      "maxDuration": 30
    },
    "api/stream/create-channel.ts": {
      "maxDuration": 30
    },
    "api/stream/get-posts.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    { "source": "/api/:match*", "destination": "/api/:match*" },
    { "source": "/(.*)", "destination": "/" }
  ]
}